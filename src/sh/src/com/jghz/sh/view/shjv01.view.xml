              <ClientEvent name="onSubControlAction">var code = arg.code;&#xD;
&#xD;
switch (code) {&#xD;
    case &quot;+&quot;:{&#xD;
        view.get(&quot;#dlSh01b&quot;).show();&#xD;
        &#xD;
        view.set((&quot;#dgSh01b.selection&quot;), view.get(&quot;#dgSh01b&quot;).getCurrentItem());&#xD;
        //得到主表的数据集对象&#xD;
        var sh01Entity = this.get(&quot;#dsSh01&quot;).getData(&quot;#&quot;);&#xD;
        &#xD;
        //根据主表的数据集得到其中的那个reference对应的数据集&#xD;
        var dsTbsh01b = this.get(&quot;#dsSh01b&quot;);&#xD;
        &#xD;
        //根据得到的数据集向其中插入一条对象，注意的是：这里的deptId不是数据库中的字段，而是实体类的属性&#xD;
        dsTbsh01b.insert({&#xD;
            compId: sh01Entity.get(&quot;compId&quot;),&#xD;
            compCode: sh01Entity.get(&quot;compCode&quot;),&#xD;
            stationcode: sh01Entity.get(&quot;stationcode&quot;),&#xD;
            transport: sh01Entity.get(&quot;transport&quot;)&#xD;
        });&#xD;
        view.set((&quot;#dgSh01b.selection&quot;), view.get(&quot;#dgSh01b&quot;).getCurrentItem());&#xD;
        //防止系统自动的添加一行&#xD;
        arg.processDefault = false;&#xD;
        break;&#xD;
    }&#xD;
    case &quot;-&quot;:{&#xD;
        var dataGrid = view.get(&quot;#dgSh01b&quot;);//获取数据表格控件&#xD;
        var selEntitys = dataGrid.get(&quot;selection&quot;);//获取所有被勾选的实体集合entityList&#xD;
        var action = view.get(&quot;#uaSh01b&quot;)&#xD;
        if (selEntitys.length > 0) {&#xD;
            dorado.MessageBox.confirm(&quot;您真的想删除当前数据吗?&quot;, function(){&#xD;
                selEntitys.each(function(select){&#xD;
                    select.remove();&#xD;
                })&#xD;
                action.execute(function(){&#xD;
                    view.get(&quot;#uaSh01b&quot;).set(&quot;successMessage&quot;, &quot;删除成功!&quot;);&#xD;
                });&#xD;
            });&#xD;
        }&#xD;
        if (selEntitys.length &lt; 1) {&#xD;
            view.get(&quot;#msgb&quot;).set(&quot;text&quot;, &quot;消息提示：请至少勾选一条数据！&quot;);&#xD;
            &#xD;
        }&#xD;
        arg.processDefault = false;&#xD;
        break;&#xD;
        &#xD;
    }&#xD;
}&#xD;
